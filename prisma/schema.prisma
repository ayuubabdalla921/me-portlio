generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  description String
  techStack   String[]
  liveLink    String
  githubLink  String
  image       String
  createdAt   DateTime @default(now())
}
